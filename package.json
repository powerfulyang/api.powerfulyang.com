  "license": "MIT",
  "author": "powerfulyang",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.build.json",
    "codegen": "graphql-codegen --config codegen.ts",
    "postinstall": "npx husky install && node-gyp rebuild && prisma generate",
    "lint:js": "eslint src --ext .ts libs --ext .ts --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "prod:dev": "cross-env PORT=3001 APP_ENV=prod nest start --watch",
    "qa:dev": "cross-env PORT=3001 APP_ENV=qa nest start --watch",
    "start": "node dist/src/main.js",
    "test:e2e": "jest --testRegex \"e2e\"",
    "test:unit": "jest --testPathIgnorePatterns \"e2e\"",
    "tsc-check": "tsc --noEmit"
  },
