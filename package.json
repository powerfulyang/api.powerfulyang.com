{
  "name": "api.powerfulyang.com",
  "version": "1.0.0",
  "private": true,
  "description": "all in one api!!!",
  "author": "powerfulyang",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/powerfulyang/api.powerfulyang.com.git",
    "type": "git",
    "name": "api.powerfulyang.com",
    "owner": "powerfulyang"
  },
  "scripts": {
    "update:lock": "pnpm i --lockfile-only",
    "bootstrap": "pnpm install --frozen-lockfile",
    "prettier": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "lint:js": "eslint src --ext .ts libs --ext .ts --fix",
    "qa:dev": "cross-env PORT=3001 APP_ENV=qa nest start --watch",
    "prod:dev": "cross-env PORT=3001 APP_ENV=prod nest start --watch",
    "start:prod": "node dist/src/main.js",
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.build.json",
    "test": "jest",
    "test:cov": "jest --coverage",
    "postinstall": "npx husky install && node-gyp rebuild",
    "tsc-check": "tsc --noEmit",
    "precommit": "lint-staged && npm run tsc-check"
  },
  "dependencies": {
    "@elastic/elasticsearch": "8.5.0",
    "@fastify/cookie": "8.3.0",
