    "@nestjs/schedule": "2.2.0",
    "@nestjs/swagger": "6.2.1",
    "@nestjs/typeorm": "9.0.1",
    "@powerfulyang/node-utils": "1.3.1",
    "@powerfulyang/utils": "1.6.0",
    "@waylaidwanderer/chatgpt-api": "1.17.3",
    "amqp-connection-manager": "4.1.10",
    "amqplib": "0.10.3",
    "bindings": "1.5.0",
    "chalk": "4.1.2",
    "class-validator": "0.14.0",
