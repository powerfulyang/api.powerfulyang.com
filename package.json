{
  "name": "api.powerfulyang.com",
  "version": "1.0.0",
  "private": true,
  "description": "all in one api!!!",
  "repository": {
    "type": "git",
    "url": "https://github.com/powerfulyang/api.powerfulyang.com.git",
    "name": "api.powerfulyang.com",
    "owner": "powerfulyang"
  },
  "license": "MIT",
  "author": "powerfulyang",
  "scripts": {
    "prebuild": "rimraf dist",
    "deprecated:build": "nest build --webpackPath webpack-build.config.js -p tsconfig.build.json",
    "build": "nest build -p tsconfig.build.json",
    "postbuild": "pnpm run check:download:traineddata",
    "check:download:traineddata": "node ./scripts/check-download-traineddata.mjs",
    "download:traineddata": "jest tools.service.spec.ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "postinstall": "npx husky install && node-gyp rebuild && prisma generate",
    "lint:js": "eslint src --ext .ts --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "deprecated:prod:dev": "cross-env PORT=3001 APP_ENV=prod nest build --webpackPath webpack-hmr.config.js --watch",
    "prod:dev": "cross-env PORT=3001 APP_ENV=prod nest start --watch",
    "deprecated:qa:dev": "cross-env PORT=3001 APP_ENV=qa nest build --webpackPath webpack-hmr.config.js --watch",
    "qa:dev": "cross-env PORT=3001 APP_ENV=qa nest start --watch",
    "start": "node dist/main.js",
    "test:e2e": "jest --testRegex \"e2e\"",
    "test:unit": "jest --testPathIgnorePatterns \"e2e\"",
    "tsc-check": "tsc --noEmit",
    "swagger-helper": "cross-env IGNORE_METADATA=true npx ts-node src/generate-metadata.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@as-integrations/fastify": "2.1.1",
